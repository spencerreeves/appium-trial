{
  "compilerOptions": {
    "sourceMap": false,
    "target": "es2020",
    "module": "NodeNext",
    "moduleResolution": "Node16",
    "removeComments": true,
    "noImplicitAny": true,
    "strictPropertyInitialization": true,
    "strictNullChecks": true,
    "types": [
      "node",
      "@wdio/types",
      "@wdio/globals/types",
      "expect-webdriverio",
    ]
  },
  "include": [
    "./config/**/*.ts",
    "./tests/**/*.ts"
  ]
}